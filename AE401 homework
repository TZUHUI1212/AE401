meaning = []
word = int(input("輸入這個語言"))
words = int(input("輸入這些語言"))
for i in  range(word):
    mean = int(input("輸入這種語言"))

"english" = "中文"
"chinese" = "英文"
"中文" = "english"  
"英文" = "chinese"
for i in meaning
   if "english":
      print"中文"
   if "chinese":
      print"英文"
   if "中文":    
      print"english"
   if "英文":    
       print"chinese"
